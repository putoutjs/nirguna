{
    "rules": {
        "typescript/find-file": "off"
    },
    "match": {
        ".github/**/*.yml": {
            "github/set-node-versions": ["on", {
                "versions": [
                    "24.x",
                    "25.x"
                ]
            }],
            "remove-useless-escape": "off"
        }
    },
    "ignore": ["example"]
}
